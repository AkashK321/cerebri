config PROTOBUF_ETHERNET
	bool "protobuf ethernet support"
	help
	  This option enables the the protobuf ethernet library
	depends on CONFIG_NETWORKING
	depends on CONFIG_NET_SOCKETS
	depends on CONFIG_NANOPB
	depends on CONFIG_TINYFRAME
	depends on CONFIG_SYNAPSE_PB
